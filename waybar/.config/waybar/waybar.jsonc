// -*- mode: json -*-
{
  "height": 26,
  "layer": "top",
  "position": "top",
  "modules-left": [
    "clock#1",
    "clock#2",
    "clock#3"
  ],
  "modules-center": [
    "group/profile"
  ],
  "image#shoaki": {
    "path": "/home/soraa/.dotfiles/img/icons/sk.webp",
    "size": 32
  },
  "group/profile": {
    "orientation": "vertical",
    "modules": [
      "hyprland/workspaces"
    ]
  },
  "custom/name": {
    "exec": "whoami",
    "format": "{text}"
  },
  "modules-right": [
    "tray",
    "pulseaudio",
    "memory",
    "cpu",
    "battery",
    "disk",
    "network"
  ],
  "pulseaudio": {
    "format": "{icon}  {volume}%",
    "format-bluetooth": "{volume}% ïŠ”",
    "format-muted": "ï€¦",
    "format-icons": {
      "alsa_output.pci-0000_00_1f.3.analog-stereo": "îŒ­",
      "alsa_output.pci-0000_00_1f.3.analog-stereo-muted": "î»¨îŒ­",
      "phone": "ï‚•",
      "phone-muted": "î´—",
      "portable": "ï‚•",
      "default": [
        "ï€§",
        "ï€¨"
      ]
    },
    "scroll-step": 1,
    "on-click": "pavucontrol"
  },
  "network": {
    "interface": "wlan*",
    "tooltip-format": "{essid}",
    "format-wifi": "ï‡«  {signalStrength}% ",
    "format-ethernet": "{ifname} ïƒ",
    "format-disconnected": "ï²",
    "tooltip-format-wifi": "{essid} ({signalStrength}%) ï‡«",
    "tooltip-format-ethernet": "{ifname} ïƒ",
    "tooltip-format-disconnected": "Disconnected",
    "max-length": 50
  },
  "hyprland/workspaces": {
    "persistent-workspaces": {
      "1": [],
      "2": [],
      "3": [],
      "4": [],
      "5": []
    },
    "format": "{icon}",
    "format-icons": {
      "1": "ğŸœ",
      "2": "ğŸœ‚",
      "3": "ğŸœ„",
      "4": "ğŸœ",
      "5": "ğŸœƒ"
    },
    "seperate-outputs": true,
    "all-outputs": true,
    "show-special": true,
    "sort-by-number": true,
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1"
  },
  "hyprland/window": {
    "seperate-outputs": true
  },
  "clock#1": {
    "format": "{:%a}",
    "tooltip": false
  },
  "clock#2": {
    "interval": 1,
    "format": "{:%I:%M:%S %p}",
    "tooltip": false
  },
  "clock#3": {
    "format": "{:%m-%d}",
    "tooltip": false
  },
  "memory": {
    "interval": 5,
    "format": "RAM:{}%"
  },
  "cpu": {
    "interval": 5,
    "format": "CPU:{usage:2}%"
  },
  "battery": {
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "{icon} {capacity}%",
    "format-icons": [
      "ï‰„",
      "ï‰ƒ",
      "ï‰‚",
      "ï‰",
      "ï‰€"
    ]
  },
  "disk": {
    "interval": 5,
    "format": "Disk:{percentage_used:2}%",
    "path": "/"
  },
  "tray": {
    "icon-size": 20
  }
}
